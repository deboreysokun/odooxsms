<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
     <record id="view_hotel_folio_report_wizard_inherit" model="ir.ui.view">
        <field name="name">folio.report.wizard.inherit</field>
        <field name="model">folio.report.wizard</field>
        <field name="inherit_id" ref="hotel.view_hotel_folio_wizard" />
        <field name="arch" type="xml">
            <field name="date_start" position="before">
                <field name="report_date" required="1"/>
            </field>
            <field name="date_start" position="attributes">
                <attribute name="invisible">1</attribute>
                <attribute name="required">0</attribute>
            </field>
            <field name="date_end" position="attributes">
                <attribute name="invisible">1</attribute>
                <attribute name="required">0</attribute>
            </field>

        </field>
     </record>
</odoo>
